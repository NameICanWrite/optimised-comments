{
  "name": "cgs-team-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "install-all": "concurrently \"cd server && npm install\" \"cd client && npm install\"",
    "dev": "concurrently \"npm start --prefix client\" \"npm run server --prefix server\""
  },
  "devDependencies": {
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.2",
    "eslint-plugin-prettier": "^4.2.1",
    "lerna": "^4.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "tsc-silent": "^1.2.2"
  }
}
